<div class="space-y-2 text-lg font-medium text-gray-800">
    <!-- Bouton pour afficher les cours disponibles -->
    <button onclick="toggleContent('cours-disponibles')" 
            class="flex items-center gap-2 hover:text-blue-600 cursor-pointer">
      <span>üìò</span>Le cours:
    </button>
    <div id="cours-disponibles" class="hidden pl-5">
      <ul class="list-disc">
        <% if (cours && cours.length > 0) { %>
          <% cours.forEach(function(coursItem) { %>
            <li>
              <a href="<%= coursItem.link %>" class="text-blue-500 hover:underline">
                <%= coursItem.name %>
              </a>
            </li>
          <% }); %>
        <% } else { %>
          <li>Aucun cours disponible.</li>
        <% } %>
      </ul>
    </div>
